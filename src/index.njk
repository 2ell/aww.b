---
layout: base.njk
title: 'aww baby'
eleventyNavigation:
  key: home
eleventyExcludeFromCollections: true
---
<div class="body-content">
<!-- post list -->
<ul class="post__collection" aria-description="List of recent blog posts in reverse-chronological order">

{%- for post in collections.post -%}

  <!-- post list item -->
  <li>
    <div class="li-container">
    <!-- post datestamp -->
     <div class="li-datetime">
    <span>{{ post.date | formatDate("MMM DD, YYYY") }}</span>
     </div>
    <!-- post title -->
     <div class="li-posttitle">
    <h2><a href="{{ post.url }}">{{ post.data.title }}</a></h2>
     </div>
</div>
  <!-- post list item END-->
  </li>

{%- endfor -%}

<!-- post list end -->
</ul>

</div>